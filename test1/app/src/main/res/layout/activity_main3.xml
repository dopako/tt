<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <Button
    android:id="@+id/backButton2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="228dp"
    android:text="@string/backButton"
    android:textColorHint="@color/white"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.498"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/viewFirmName"
    app:layout_constraintVertical_bias="0.863" />

  <TextView
    android:id="@+id/viewCarNum"
    android:layout_width="222dp"
    android:layout_height="35dp"
    android:layout_marginStart="92dp"
    android:layout_marginTop="36dp"
    android:textAlignment="center"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <TextView
    android:id="@+id/viewOwnerName"
    android:layout_width="222dp"
    android:layout_height="35dp"
    android:layout_marginStart="92dp"
    android:layout_marginTop="32dp"
    android:textAlignment="center"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/viewCarNum" />

  <TextView
    android:id="@+id/viewPrice"
    android:layout_width="222dp"
    android:layout_height="35dp"
    android:layout_marginStart="92dp"
    android:layout_marginTop="36dp"
    android:textAlignment="center"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/viewOwnerName" />

  <TextView
    android:id="@+id/viewDescr"
    android:layout_width="222dp"
    android:layout_height="35dp"
    android:layout_marginStart="92dp"
    android:layout_marginTop="32dp"
    android:textAlignment="center"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/viewPrice" />

  <TextView
    android:id="@+id/viewPhoneNum"
    android:layout_width="222dp"
    android:layout_height="35dp"
    android:layout_marginStart="92dp"
    android:layout_marginTop="32dp"
    android:textAlignment="center"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/viewDescr" />

  <TextView
    android:id="@+id/viewFirmName"
    android:layout_width="222dp"
    android:layout_height="35dp"
    android:layout_marginStart="92dp"
    android:layout_marginTop="32dp"
    android:textAlignment="center"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/viewPhoneNum" />

</androidx.constraintlayout.widget.ConstraintLayout>